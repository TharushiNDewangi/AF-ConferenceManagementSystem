Constansts.GET_ALL_CATEGORIES_SUCCESS:\r\n            state = {\r\n                ...state,\r\n                categories: action.payload.categories \r\n            }\r\n            break;\r\n        case categoryConstansts.ADD_NEW_CATEGORY_REQUEST:\r\n            state = {\r\n                ...state,\r\n                loading: true\r\n            }\r\n            break;\r\n        case categoryConstansts.ADD_NEW_CATEGORY_SUCCESS:\r\n            const category = action.payload.category;\r\n            const updatedCategories = buildNewCategories(category.parentid,state.categories, category);\r\n\r\n            console.log('updated categories',updatedCategories);\r\n            state = {\r\n                ...state,\r\n                categories: updatedCategories,\r\n                loading: false\r\n            }\r\n            break;\r\n        case categoryConstansts.ADD_NEW_CATEGORY_FAILURE:\r\n            state = {\r\n                ...initState,\r\n\r\n            }\r\n            break;\r\n    }\r\n\r\n    return state;\r\n}"},"lineCount":null}},"error":null,"hash":"dc233ca365119139de85f82ab32a7324","cacheData":{"env":{}}}